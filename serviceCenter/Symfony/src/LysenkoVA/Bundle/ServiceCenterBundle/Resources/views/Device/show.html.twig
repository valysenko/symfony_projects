{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Device</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Serialnumber</th>
                <td>{{ entity.serialNumber }}</td>
            </tr>
            <tr>
                <th>Model</th>
                <td>{{ entity.model }}</td>
            </tr>
            <tr>
                <th>Numberofmodel</th>
                <td>{{ entity.numberOfModel }}</td>
            </tr>
            <tr>
                <th>Cenditionofdevice</th>
                <td>{{ entity.cenditionOfDevice }}</td>
            </tr>
            <tr>
                <th>Warranty</th>
                <td>{{ entity.warranty }}</td>
            </tr>
            <tr>
                <th>Propertyofsc</th>
                <td>{{ entity.propertyOfSc }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_device') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_device_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
